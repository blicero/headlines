SELECT
    tag_id,
	COUNT(id) AS cnt
	FROM tag_link
	GROUP BY tag_id
)

SELECT
	t.id,
	t.name,
	t.description,
	t.parent,
	t.lvl,
	t.full_name,
	l.cnt
FROM tag_sorted t
INNER JOIN links l ON t.id = l.tag_id
ORDER BY full_name</sql><current_tab id="0"/></tab_sql></sqlb_project>
